#include <stdio.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 *
 */

int main(void)

	char alphabet;
char alpha;
{
	for (alphabet = 'a'; alphabet <= 'z' alphabet++)
	{
		putchar (alphabet);
	}
	for (alpha = 'A'; alpha <= 'Z' alpha++)
	{
		putchar (alpha);
	}
	putchar ('\n');

	return (0);
}
