#include "main.h"

/**
 * strncpy - copies a string
 * @dest: The destination string
 * @src: The source string
 * @n: number of bytes to be copid from src
 *
 * Return: pointer to dest
 */

char *_strncpy(char *dest, char *src, int n)
{
	int  x;

	for(x = 0; x < n && src[x] != '\0'; x++)
	{
		dest[x] = src[x];
	}
	for (; x < n; x++)
	{
		dest = '\0';
	}
	return 0;
}
