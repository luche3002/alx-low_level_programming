#include "main.h"

/**
 * main - print the program's name
 * @argc: the number of command line arguments
 * @argv: an array of command line arguments
 *
 * Return: Always 0 (SUCCESS)
 */

int main(int argc, char *argv[])
{
	int a = 0;
	(void)argc;

	while (argv[0][a] != '\0')
	{
		_putchar(argv[0][a]);
		a++;
	}
	_putchar ('\n');

	return (0);
}
